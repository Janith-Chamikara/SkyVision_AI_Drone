{
    "video": {
        "source": "WEB_CAM",
        "width": 640,
        "height": 480,
        "fps": 30,
        "focal_length_x": 1,
        "focal_length_y": 1
    },
    "model": {
        "input_width": 640,
        "input_height": 192
    },
    "point_cloud": {
        "enabled": true,
        "window_width": 1024,
        "window_height": 768,
        "point_size": 4.0,
        "background_color": [0.1, 0.1, 0.1],
        "depth_scale": 30.0,
        "min_depth": 0.1,
        "max_depth": 5.0,
        "subsample_factor": 4,
        "focal_scale": 1.5
    },
    "visualization": {
        "show_coordinate_frame": true,
        "show_2d_display": true
    }
}
